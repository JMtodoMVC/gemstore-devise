<!DOCTYPE html>
<html ng-app="gemStoreApp">
  <head>
    <title>Railsy Gemstore</title>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>
  </head>

  <body>
    <div ng-controller="StoreController as store" ng-init="index()">
    <%= render 'layouts/angular' %>
    <% yield %>

    </div>
  </body>
</html>
